<%= form_for @recipe do |r| %>
<h1> Recipe </h1>
<%= r.label :title %>
<%= r.text_field :title %>
<h1> Ingredients </h1>
  <%= r.fields_for :ingredients do |i| %>
    <%= i.label :name %>
    <%= i.text_field :name %>
    <%= i.label :quantity %>
    <%= i.text_field :quantity %></br>
  <% end %>
<%= r.submit %>
<% end %>
